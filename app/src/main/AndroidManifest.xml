<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.steffen.nameapp">
    <uses-permission android:name="android.permission.CAMERA" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
                >
        <activity android:name=".Activities.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />

            </intent-filter>
        </activity>
        <activity android:name=".Activities.PictureActivity" android:parentActivityName=".Activities.MainActivity"></activity>
        <activity android:name=".Activities.addActivity" android:parentActivityName=".Activities.MainActivity"></activity>
        <activity android:name=".Activities.gameActivity" android:parentActivityName=".Activities.MainActivity"></activity>
        <activity android:name=".Activities.NamesActivity" android:parentActivityName=".Activities.MainActivity"></activity>
        <activity android:name=".Activities.UserPrefs" android:label="UserPrefs"></activity>
        <activity android:name=".Activities.PrefPic" android:parentActivityName=".Activities.MainActivity"></activity>
    </application>


</manifest>